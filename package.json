    "jwt-decode": "^2.2.0",
    "link": "^0.1.5",
    "moment": "^2.24.0",
    "next-frame": "^0.2.3",
    "path": "^0.12.7",
    "performance-now": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-native": "0.68.5",
    "react-native-action-sheet": "^2.2.0",
    "react-native-background-fetch": "^4.0.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-haptic-feedback": "^1.13.1",
